

if _G.Bosslvl == 2 then
	if game.PlaceId == 7592937982 then

		wait(3)
		local args = {
			[1] = (_G.form)
		}

		game:GetService("ReplicatedStorage").Remotes.Form_Selected:InvokeServer(unpack(args))

		_G.FarmOn = true --TRUE to ENABLE / FALSE to DISABLE
		_G.Target = "Boss" --Enemy's Name

		local plr=game.Players.LocalPlayer;local char=plr.Character;spawn(function()
			while _G.FarmOn and wait() do
				for i,v in pairs(game:GetService("Workspace").NPC:GetChildren()) do
					if string.match(v.Name,_G.Target) and v.Humanoid.Health ~= 0 then
						repeat wait() pcall(function()
								-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = true,
    [2] = "GolpeIzquierdo",
    [3] = game:GetService("ReplicatedStorage").Asset.Anims.L
}

game:GetService("ReplicatedStorage").Remotes.Golpe:FireServer(unpack(args))


								char.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame + v.HumanoidRootPart.CFrame.lookVector * -5
								game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(char.HumanoidRootPart.Position, v.HumanoidRootPart.Position)
							end) until _G.FarmOn == false
					end
				end
			end
		end)
	else
		game:GetService("TeleportService"):Teleport(7592937982)
	end
